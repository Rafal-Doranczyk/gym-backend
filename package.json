{
  "name": "gym-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit",
    "build": "esbuild `find src \\( -name '*.ts' \\)` --platform=node --outdir=build --resolve-extensions=.js",
    "build:docker:prod": "docker build . -t my-fastify-app --build-arg APP_ENV=production",
    "start": "node build",
    "dev": "tsx watch src .env | pino-pretty --colorize",
    "pretest": "npm run build",
    "test": "vitest",
    "test:watch": "vitest -w"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.2",
    "@sinclair/typebox": "^0.29.4",
    "ajv": "^8.12.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.19.2",
    "fastify-plugin": "^4.5.0",
    "gym-shared": "^0.0.24"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.18.13",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "inversify": "^6.0.1",
    "pino-pretty": "^10.0.1",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
